<!doctype html>
<html>
    <head>
	<meta charset="utf8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>The Bros Home</title>
	<link rel="stylesheet" type="text/css" href="style.css">
    </head>
    <body>
	<main class="login-page">
	    <section class="login-area centered-form">
		<div>
		    <form>
			<input class="username-input" type="text" placeholder="Username">
			<input class="password-input" type="password" placeholder="Password">
			<button class="primary">Log In</button>
		    </form>
		</div>
		<div>
		    <p class="registration-text">Not registred? <a href="/signup">Create an account</a></p>
		</div>
	    </section>
	</main>
    </body>
</html>
